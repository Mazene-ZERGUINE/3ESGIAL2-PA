fun scrapAllTitles(content)
    start
        data = content::get_all_titles();

        allTitles = data::fetch_all();
        size = allTitles::capacity();
        
        print("all data") ; 

        i = 0; 
        while(i < size) { 
            print(allTitles{i};);
            i++; 
        }

        print("one element")
        allTitles::get_one("id")  /** en cours de construction */ 
    end



